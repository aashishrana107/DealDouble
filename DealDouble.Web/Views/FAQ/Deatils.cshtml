@model DealDouble.Web.ViewModels.FeedbackViewModel
@{
    ViewBag.Title = "Feedback";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}


<div class="row">
    <<div class="col-lg-12 m-2">
        <div class="float-left">
            <h2>Feedback</h2>
        </div>
        
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>
                        Email
                    </th>
                    <th>
                        Subject
                    </th>
                    <th>
                        Message
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var category in Model.feedback)
                {
                    <tr>
                        <td>
                            @category.Email
                        </td>
                        <td>
                            @category.Subject
                        </td>
                        <td>
                            @category.Message
                        </td>
                        
                    </tr>
                }
            </tbody>
        </table>
